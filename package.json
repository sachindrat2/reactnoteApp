{
  "name": "notesapp",
  "private": true,
  "version": "2.0.1",
  "type": "module",
  "homepage": "https://sachindrat2.github.io/reactnoteApp",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:prod": "vite build --mode production",
    "preview": "vite preview",
    "preview:dist": "vite preview --port 4173",
    "predeploy": "npm run build:prod",
    "deploy": "gh-pages -d dist",
    "deploy:prod": "npm run build:prod && npm run deploy",
    "analyze": "npm run build && npx vite-bundle-analyzer dist/assets/*.js",
    "clean": "rm -rf dist node_modules/.vite",
    "test:build": "npm run build && npm run preview"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "http-proxy-middleware": "^3.0.5",
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^16.1.5",
    "react-quill": "^2.0.0",
    "react-router-dom": "^7.9.4"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "gh-pages": "^6.3.0",
    "postcss": "^8.4.27",
    "tailwindcss": "^3.3.3",
    "terser": "^5.44.0",
    "vite": "^4.4.5"
  }
}
